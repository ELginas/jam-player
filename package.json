{
  "name": "jam-player",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "start": "web-ext run --source-dir dist",

{
  "manifest_version": 2,
  "name": "Jam Player",
  "version": "1.0.0",
  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://itch.io/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["https://itch.io/"]
}
